<?xml version="1.0" encoding="utf-8" ?>
		<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
		                   assembly="DSSGenNHibernate"
		                   namespace="DSSGenNHibernate.EN.Moodle">
			<class name="AdministradorEN" table="[Administrador]">
				  	
						<id name="Id" column="[id]" type="integer">
							<generator class="identity"/>
						</id>
					<property name="Nick" type="string"> 
						<column name="[nick]"  unique="true" not-null="true"/>
					</property>
					<property name="Password" type="string"> 
						<column name="[password]"  not-null="true"/>
					</property>
					<property name="Nombre" type="string"> 
						<column name="[nombre]"  not-null="true"/>
					</property>
					<property name="Descripcion" type="string"> 
						<column name="[descripcion]"  not-null="true"/>
					</property>
			
			
			
			
			
					
					
					
					
					
		
		  </class>
		  
		  <query name="AdministradorENReadAllHQL">from AdministradorEN</query>
		</hibernate-mapping>
	